<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="RegressionSuite" parallel="none">
	<test name="shouldBeAbleToSearchForHotels">
		<classes>
			<class name="tests.HotelBookingTest">
				<methods>
					<include name="shouldBeAbleToSearchForHotels"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="shouldThrowAnErrorIfSignInDetailsAreMissing">
		<classes>
			<class name="tests.SignInTest">
				<methods>
					<include name="shouldThrowAnErrorIfSignInDetailsAreMissing"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="testThatResultsAppearForAOneWayJourney">
		<classes>
			<class name="tests.FlightBookingTest">
				<methods>
					<include name="testThatResultsAppearForAOneWayJourney"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite>
